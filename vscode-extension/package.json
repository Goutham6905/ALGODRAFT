{
  "name": "algodraft-ai",
  "displayName": "AlgoDraft AI",
  "publisher": "algodraft",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "algodraft.openSidebar",
        "title": "AlgoDraft: Open Sidebar"
      },
      {
        "command": "algodraft.analyzeSelection",
        "title": "AlgoDraft: Analyze Selected Code"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "algodraft.analyzeSelection",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "algodraft",
          "title": "AlgoDraft",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "algodraft": [
        {
          "type": "webview",
          "id": "algodraft.sidebar",
          "name": "AlgoDraft",
          "icon": "media/icon.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p .",
    "watch": "tsc -w",
    "package": "vsce package",
    "pretest": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^2.22.0",
    "typescript": "^4.9.5"
  }
}